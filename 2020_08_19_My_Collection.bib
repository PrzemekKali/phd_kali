journal = {Materials Science Forum},
keywords = {Damage detection,Defect,Finite-element method (FEM) reinforcement,Mode shape,Mode shapes,Natural frequency,Numerical experiment,Slabs,Structural engineering,Structural health monitoring,Structural vibration,Vibration based damage detection},
language = {en},
month = {jul},
pages = {178--183},
title = {{Vibration-based damage detection techniques for health monitoring of construction of a multi-storey building}},
url = {https://www.scientific.net/MSF.931.178 http://files/452/Shatilov i Lyapin - 2018 - Vibration-Based Damage Detection Techniques for He.pdf},

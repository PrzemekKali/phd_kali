issn = {18737323},
journal = {Engineering Structures},
keywords = {Dimensionless description,Indirect identification,Subspace identification,Vehicle-bridge interaction},
number = {January},
pages = {111688},
publisher = {Elsevier Ltd},
title = {{Application of short-time stochastic subspace identification to estimate bridge frequencies from a traversing vehicle}},
url = {https://doi.org/10.1016/j.engstruct.2020.111688},
volume = {230},
year = {2021}

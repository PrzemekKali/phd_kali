number = {4},
pages = {361--376},
title = {{Finite element model updating using fish school search and volitive particle swarm optimization}},
url = {http://www.medra.org/servlet/aliasResolver?alias=iospress&doi=10.3233/ICA-150495 http://files/557/Boulkaibet i in. - 2015 - Finite element model updating using fish school se.pdf},

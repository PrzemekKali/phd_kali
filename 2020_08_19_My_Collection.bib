@book{Miettinen1999,
abstract = {A large number of papers devoted to multiobjective optimization is still not well balanced by a sufficient quantity of books which can serve as a reference for the field. Therefore, the monograph by Kaisa Miettinen, a concise (as compared to the extent of the subject), formal, and clearly written text, should be warmly welcomed. It is a presentation of fundamentals and cornerstone developments of multiobjective optimization. It also reports on some of the author's own contributions to the field. $\backslash$par The book is composed of nine chapters. Chapter One presents basic concepts of the theory of multiobjective optimization. Chapter Two lists the major methods available in the field. Each method is briefly described, together with some theoretical results and underlying concepts, applications, extensions. This part can effectively be used as a basis for a postgraduate course. $\backslash$par Chapter Three contains a general description of some of the most popular software packages for solving multiobjective optimization problems. The description is general and does not cover implementation details. In the next chapter, Chapter Four, an important subject is treated, namely methods for graphical visualization of the search process for the final efficient solution among the universe of (efficient) solutions. Chapter Five contains some guidelines on how to select the most suitable method to solve a problem. Results of numerical testing of two selected methods, namely the Geoffrion-Dyer-Feinberg method and the Nimbus method, the latter an original method proposed by the author, are reported in Chapter Six. $\backslash$par The reader can get the flavor of multiobjective optimization applications in Chapter Seven, where two practical problems, the optimal control of elastic spring and the optimal control of a continuous steel casting process, are presented and solved. Some comments (Chapter Eight) on future research directions and general remarks (Chapter Nine) conclude the work. $\backslash$par This useful reference book certainly will find its way to the bookshelves of many, whose interests range from vector optimization (hard mathematics) through decision theory (behavioral sciences) to decision making in the tough world of reality.},
annote = {Accession Number: MR1784937; Authors: 338986 Author Affiliation: FIN-JVS; Department of Mathematics, University of Jyv{\"{a}}skyl{\"{a}}. Copyright: {\textcopyright}Copyright 2021, American Mathematical Society. Document Type: Book. Publication Type: Book Date: 19990101. Pages: xxii+298{\~{}}pp},
author = {Miettinen, Kaisa},
editor = {Kaliszewski, Ignacy},
isbn = {0-7923-8278-1},
keywords = {65K05 Numerical analysis -- Mathematical programmi,90-02 Operations research,90C29 Operations research,90C30 Operations research,Numerical analysis -- Mathematical programming,Operations research,mathematical programming -- Mathematical programmi,mathematical programming -- Research exposition,optimization and variational techniques -- Mathema},
language = {English},
month = {jan},

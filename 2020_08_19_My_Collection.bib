issn = {08883270},
journal = {Mechanical Systems and Signal Processing},
keywords = {Bridges,Concrete,Dynamic tests,Finite element models,Operational modal analysis,Structural identification},
language = {en},
month = {apr},
pages = {84--107},
publisher = {Elsevier},
title = {{Vibration analysis and structural identification of a curved multi-span viaduct}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0888327014003240 http://files/1172/Benedettini et al. - 2015 - Vibration analysis and structural identification o.pdf http://dx.doi.org/10.1016/j.ymssp.2014.08.008},

file = {:C$\backslash$:/Users/PRZKALIT/Documents/Mendeley Desktop/Kye, Jung, Jung - 2019 - Experimental Investigation on a Cable Structure Equipped with an Electrodynamic Damper and Its Monitoring Strat.pdf:pdf},

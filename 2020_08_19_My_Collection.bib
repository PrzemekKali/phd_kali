journal = {Mechanical Systems and Signal Processing},
keywords = {Modal parameters,Stay cables,Subspace method,Wavelet transform},
language = {en},
month = {apr},
number = {1},
pages = {133--150},
publisher = {Elsevier},
title = {{Modal parameter identification of stay cables from output-only measurements}},
volume = {25},

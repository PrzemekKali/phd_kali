journal = {Journal of Civil Structural Health Monitoring},
keywords = {Cable-stayed bridges,Digital image processing,Feature-based detection,Non-contact vision-based system,Stay cables,Tensile forces},
number = {1},
pages = {69--84},
publisher = {Springer Berlin Heidelberg},
title = {{Cable tension monitoring through feature-based video image processing}},
volume = {11},
year = {2021}

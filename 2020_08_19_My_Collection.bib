@article{Shahabpoor2018,
abstract = {The interaction of walking people with large vibrating structures, such as footbridges and ﬂoors, in the vertical direction is an important yet challenging phenomenon to describe mathematically. Several different models have been proposed in the literature to simulate interaction of stationary people with vibrating structures. However, the research on moving (walking) human models, explicitly identiﬁed for vibration serviceability assessment of civil structures, is still sparse. In this study, the results of a comprehensive set of FRFbased modal tests were used, in which, over a hundred test subjects walked in different group sizes and walking patterns on a test structure. An agent-based model was used to simulate discrete trafﬁc-structure interactions. The occupied structure modal parameters found in tests were used to identify the parameters of the walking individual's singledegree-of-freedom (SDOF) mass-spring-damper model using ‘reverse engineering' methodology. The analysis of the results suggested that the normal distribution with the average of l = 2.85Hz and standard deviation of r = 0.34Hz can describe human SDOF model natural frequency. Similarly, the normal distribution with l = 0.295 and r = 0.047 can describe the human model damping ratio. Compared to the previous studies, the agent-based modelling methodology proposed in this paper offers signiﬁcant ﬂexibility in simulating multi-pedestrian walking trafﬁcs, external forces and simulating different mechanisms of human-structure and human-environment interaction at the same time.},
author = {Shahabpoor, Erfan and Pavic, Aleksandar and Racic, Vitomir},
doi = {10.1016/j.ymssp.2017.10.028},
file = {:C\:/Users/przem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Shahabpoor, Pavic, Racic - 2018 - Identification of walking human model using agent-based modelling.pdf:pdf},
issn = {08883270},
journal = {Mechanical Systems and Signal Processing},
keywords = {Discrete traffic model,FRF-based modal testing,Human-structure interaction,SDOF moving human model,Vibration serviceability},
language = {en},
month = {apr},
pages = {352--367},
publisher = {The Authors},
title = {{Identification of walking human model using agent-based modelling}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0888327017305599 http://files/1186/Shahabpoor et al. - 2018 - Identification of walking human model using agent-.pdf https://doi.org/10.1016/j.ymssp.2017.10.028},

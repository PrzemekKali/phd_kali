publisher = {Elsevier B.V.},
title = {{A parallel multi-objective particle swarm optimization for cascade hydropower reservoir operation in southwest China}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S156849461830334X http://files/593/Niu i in. - 2018 - A parallel multi-objective particle swarm optimiza.pdf https://doi.org/10.1016/j.asoc.2018.06.011},

keywords = {Frequency domain,Integration,Signal processing,Time series,Windowed overlap-add},
language = {en},
month = {oct},
pages = {511--519},
publisher = {Elsevier Ltd},
title = {{Integrating time signals in frequency domain – Comparison with time domain integration}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0263224114003832 http://files/750/Brandt i Brincker - 2014 - Integrating time signals in frequency domain – Com.pdf http://dx.doi.org/10.1016/j.measurement.2014.09.004},

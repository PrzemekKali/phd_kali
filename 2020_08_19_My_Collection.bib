journal = {Mechanical Systems and Signal Processing},
keywords = {Control,MR damper,Optimisation,Semi-active,Tuned mass damper,Vibration},
language = {en},
month = {apr},
pages = {707--731},
title = {{Optimised semi-active tuned mass damper with acceleration and relative motion feedbacks}},
volume = {130},
year = {2019}
}

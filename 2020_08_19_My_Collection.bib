number = {8},
pages = {5883--5894},
publisher = {IEEE},
title = {{Efficiency Estimation of the Induction Machine by Particle Swarm Optimization Using Rapid Test Data With Range Constraints}},
url = {https://ieeexplore.ieee.org/document/8488699/ http://files/402/Bijan i Pillay - 2019 - Efficiency Estimation of the Induction Machine by .pdf},

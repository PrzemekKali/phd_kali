journal = {2019 International Conference on Smart Grid Synchronized Measurements and Analytics, SGSMA 2019},
keywords = {Electromechanical modes,Forced oscillations,Phasor Measurement Unit (PMU),Small signal stability,Stochastic subspace identification},
pages = {3--5},
title = {{Performance of stochastic subspace identification methods in presence of forced oscillations}},
year = {2019}
}

	keywords = {operational modal analysis, ambient vibration, Arch bridge design \& construction, Bridge design \& construction, dynamic characteristics, Finite element method, finite element model (FEM) updating, Historic bridges, historical arch bridge, modal testing, Structural analysis, Structural dynamics},
	pages = {177--185}

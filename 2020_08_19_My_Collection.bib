pmid = {15112367},
publisher = {MIT Press},
title = {{Gaussian processes for machine learning.}},
url = {http://files/734/Rasmussen i Williams - 2006 - Gaussian processes for machine learning.pdf},
volume = {14},
year = {2004}
}

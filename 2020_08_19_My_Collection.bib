author = {Klasztorny, Marian},
file = {:C$\backslash$:/Users/PRZKALIT/Documents/Mendeley Desktop/Klasztorny - 2014 - Dynamic analysis of composite beam bridges along Polish highspeed railway line ”CMK” at conditions of increased trai.pdf:pdf},

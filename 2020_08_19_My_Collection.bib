language = {en},
month = {mar},
number = {7},
pages = {1103--1117},
title = {{Seismic protection of the horizontally curved bridge with semi-active variable stiffness damper and isolation system}},
url = {http://journals.sagepub.com/doi/10.1177/1369433216634477 http://files/941/Kataria i Jangid - 2016 - Seismic protection of the horizontally curved brid.pdf},

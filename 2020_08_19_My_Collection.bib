issn = {15452255},
journal = {Structural Control and Health Monitoring},
keywords = {dynamic monitoring system,footbridges,magneto-rheological damper,passive and semiactive control,tuned mass damper},
language = {en},
month = {mar},
number = {9},
pages = {e2208},
title = {{Vibration control of a slender footbridge using passive and semiactive tuned mass dampers}},
volume = {25},
year = {2018}
}

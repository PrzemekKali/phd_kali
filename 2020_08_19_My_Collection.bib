arxivId = {2001.01797},
author = {{Sadeghi Eshkevari}, Soheil and Matarazzo, Thomas J. and Pakzad, Shamim N.},
doi = {10.1016/j.ymssp.2020.106733},
eprint = {2001.01797},
file = {:C$\backslash$:/Users/PRZKALIT/Documents/Mendeley Desktop/Sadeghi Eshkevari, Matarazzo, Pakzad - 2020 - Bridge modal identification using acceleration measurements within moving vehicles.pdf:pdf},
issn = {08883270},

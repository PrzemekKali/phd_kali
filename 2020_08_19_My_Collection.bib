isbn = {978-0-7803-7282-5},
keywords = {Acceleration,Birds,Ear,Evolutionary computation,Genetic algorithms,MOPSO,Multidimensional systems,Pareto dominance,Particle swarm optimization,Proposals,Sorting,Testing,evolutionary computation,evolutionary techniques,flight direction,heuristic,heuristic programming,multiobjective optimization,operations research,optimisation,particle swarm optimization},
language = {en},

	author = {Moncayo, Hever and Marulanda, Johannio and Thomson, Peter},
	month = apr,
	year = {2010},
	keywords = {Modal analysis, Aerospace industries, Aircraft industry, Airplanes, Flight, Methodology, Structural engineering, Vibration (Aeronautics)},
	pages = {99--104},
	file = {Moncayo i in. - 2010 - Identification and Monitoring of Modal Parameters .pdf:C\:\\Users\\KBP\\Zotero\\storage\\8TREBX3R\\Moncayo i in. - 2010 - Identification and Monitoring of Modal Parameters .pdf:application/pdf}

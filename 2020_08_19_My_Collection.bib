doi = {10.1016/j.engstruct.2018.10.031},
file = {:C$\backslash$:/Users/PRZKALIT/Documents/Mendeley Desktop/Naito, Bolander - 2019 - Damage detection method for RC members using local vibration testing.pdf:pdf},
issn = {18737323},
journal = {Engineering Structures},
keywords = {Carbon fiber sheet,Concrete,Damage detection,Nondestructive testing,Resonant frequency,Vibration,Wave propagation analysis},
language = {en},
month = {jul},
number = {October 2018},
pages = {361--374},
publisher = {Elsevier},
title = {{Damage detection method for RC members using local vibration testing}},
volume = {178},

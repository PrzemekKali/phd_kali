doi = {10.1109/4235.985692},
file = {:C$\backslash$:/Users/PRZKALIT/Documents/Mendeley Desktop/Clerc, Kennedy - 2002 - The particle swarm-explosion, stability, and convergence in a multidimensional complex space.pdf:pdf},

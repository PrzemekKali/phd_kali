publisher = {Taylor $\backslash${\&} Francis},
title = {{Monitoring the dynamic characteristics of an urban bridge before, during and after widening}},
url = {http://www.tandfonline.com/doi/full/10.1080/15732479.2014.921827 http://files/498/Maadani i in. - 2015 - Monitoring the dynamic characteristics of an urban.pdf},

file = {:C$\backslash$:/Users/PRZKALIT/Documents/Mendeley Desktop/Sun et al. - 2019 - MULTI-OBJECTIVE OPTIMIZATION DESIGN OF MAGNETIC BEARING BASED ON GENETIC PARTICLE SWARM OPTIMIZATION.pdf:pdf},
issn = {19378726},
journal = {Progress In Electromagnetics Research M},

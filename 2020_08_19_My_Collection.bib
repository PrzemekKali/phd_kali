doi = {10.1016/j.ymssp.2020.106733},
eprint = {2001.01797},
file = {:C\:/Users/przem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sadeghi Eshkevari, Matarazzo, Pakzad - 2020 - Bridge modal identification using acceleration measurements within moving vehicles.pdf:pdf},

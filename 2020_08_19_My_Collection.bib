language = {en},
month = {jul},
number = {3},
pages = {162--172},
title = {{Assessment of Highway Bridge Upgrading by Dynamic Testing and Finite-Element Model Updating}},
url = {http://ascelibrary.org/doi/10.1061/%28ASCE%291084-0702%282003%298%3A3%28162%29 http://files/443/Brownjohn i in. - 2003 - Assessment of Highway Bridge Upgrading by Dynamic .pdf},

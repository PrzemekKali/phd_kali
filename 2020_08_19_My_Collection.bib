keywords = {Discrete traffic model,FRF-based modal testing,Human-structure interaction,SDOF moving human model,Vibration serviceability},
language = {en},
month = {apr},
pages = {352--367},
publisher = {The Authors},
title = {{Identification of walking human model using agent-based modelling}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0888327017305599 http://files/1186/Shahabpoor et al. - 2018 - Identification of walking human model using agent-.pdf https://doi.org/10.1016/j.ymssp.2017.10.028},

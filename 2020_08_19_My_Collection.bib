issn = {1757-8981, 1757-899X},
journal = {IOP Conference Series: Materials Science and Engineering},
language = {en},
month = {mar},
number = {3},
pages = {32050},
title = {{Load Testing of GFRP Composite U-Shape Footbridge}},

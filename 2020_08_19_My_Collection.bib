doi = {10.1006/jsvi.2001.3880},
file = {:C$\backslash$:/Users/PRZKALIT/Documents/Mendeley Desktop/Mevel et al. - 2002 - Merging sensor data from multiple measurement set-ups for non-stationary subspace-based modal analysis.pdf:pdf},

@article{Alaghebandha2017,
abstract = {This paper deals with the formulation of a sequencing problem with the dual goals of varying the parts utilization at different workstations of the assembly line and varying the workload associated with each workstation; these two objectives are typically inversely correlated with each other, and therefore the simultaneous optimization of both is challenging. Owing to the NP-hardness of the problem, this paper introduces a Discrete Particle Swarm Optimization (DPSO) algorithm, a Memetic Algorithm (MA), a Weighted sum Multi-Objective Genetic Algorithm (MOGAW), and a Non-dominated Sorting Genetic Algorithm (NSGA-II) to solve a just-in-time sequencing problem where these objectives are to be optimized simultaneously. The performance of these algorithms is compared against each other on small, medium, and large problems. According to statistical analysis, experimental results show that discrete particle swarm optimization outperforms the other algorithms in respect of a comparison metric.},
author = {Alaghebandha, Mohammad and Hajipour, Vahid and Hemmati, Mojtaba},
doi = {10.1080/17509653.2016.1258593},
file = {:C$\backslash$:/Users/PRZKALIT/Documents/Mendeley Desktop/Alaghebandha, Hajipour, Hemmati - 2017 - Optimizing multi-objective sequencing problem in mixed-model assembly line on just-in-time part.pdf:pdf},
issn = {17509661},
journal = {International Journal of Management Science and Engineering Management},
keywords = {DPSO,MA,MOGAW,Mixed-model sequencing,NSGA-II,desirability function,just-in-time},
language = {en},
month = {jul},
number = {4},
pages = {288--298},
publisher = {Taylor {\&} Francis},
title = {{Optimizing multi-objective sequencing problem in mixed-model assembly line on just-in-time: particle swarm optimization algorithm}},
volume = {12},
year = {2017}
}
@article{Belevicius2018,
abstract = {The article concerns optimization of network arch bridges. This is challenging optimization problem involving even for conventional scheme of network arch bridge the identification of some topological parameters as well as shape configurations and all sizing parameters of structural members, seeking the minimum weight. Optimal bridge scheme is sought tuning a large set of design parameters of diverse character: the type of hanger arrangement, the number of hangers, their inclination angles and placement distances, the arch shape and rise, etc. Mathematically, the optimization of the bridge scheme is a mixed-integer constrained global optimization problem solved employing stochastic evolutionary algorithm. Plane heavy/ moderate/and light-deck bridges of 18, 30, 42 and 54 m spans were optimized using proposed optimization technique. The decisive design parameters and their promising ranges were revealed. Also, the influence of some simplifications is shown: changing the arch shape from elliptical to circular, placing the hangers at equal distances, etc.},
author = {Belevi{\v{c}}ius, Rimantas and Juozapaitis, Algirdas and Rusakevi{\v{c}}ius, Dainius},
doi = {10.3311/PPci.10036},
file = {:C$\backslash$:/Users/PRZKALIT/Documents/Mendeley Desktop/Belevi{\v{c}}ius, Juozapaitis, Rusakevi{\v{c}}ius - 2018 - Parameter study on weight minimization of network arch bridges.pdf:pdf},
issn = {15873773},
journal = {Periodica Polytechnica Civil Engineering},
keywords = {Network arch bridge,Shape and sizing parameters,Topology,Weight minimization},
number = {1},
pages = {48--55},
title = {{Parameter study on weight minimization of network arch bridges}},
volume = {62},
year = {2018}
}

@article{Ding2018,
abstract = {In this paper, design sensitivity analysis methods for the transient response of nonviscously damped systems are considered. The damping forces of the non-viscously damped systems depend on the past history of motion via convolution integrals over some suitable kernel functions. The adjoint variable method (AVM) is adopted to develop the design sensitivity analysis. Two numerical solution schemes, namely the discretize-then-differentiate method and the differentiate-then-discretize method, are introduced to complete the AVM for the sensitivity analysis of non-viscously damped systems. The discretize-thendifferentiate AVM discretizes the equations of motion based on the Newmark-b implicit integration method ﬁrst and then differentiates the discrete equations. On the contrary, the differentiate-then-discretize AVM ﬁrstly differentiates the equations of motion after transforming it into a state-space form, and then discretizes the equations based on a modiﬁed precise integration method (PIM). The numerical accuracy, efﬁciency, consistency and implementation effort are discussed and compared. Two numerical examples are presented to show the effectiveness of both methods. The results indicate that, by considering both computational accuracy and efﬁciency, the PIM based differentiate-then-discretize method is more suitable than the Newmark-b based discretize-then-differentiate method for the sensitivity analysis of transient responses for non-viscously damped systems.},
author = {Ding, Zhe and Li, Li and Li, Xiaobai and Kong, Jianyi},
doi = {10.1016/j.ymssp.2018.03.043},
file = {:C\:/Users/przem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ding et al. - 2018 - A comparative study of design sensitivity analysis based on adjoint variable method for transient response of non-v.pdf:pdf},
issn = {08883270},
journal = {Mechanical Systems and Signal Processing},
keywords = {Adjoint variable method,Discretize-then-differentiate method,Non-viscous damping,Sensitivity analysis,Transient response},
language = {en},
month = {apr},
pages = {390--411},
title = {{A comparative study of design sensitivity analysis based on adjoint variable method for transient response of non-viscously damped systems}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0888327018301626 http://files/1158/Ding et al. - 2018 - A comparative study of design sensitivity analysis.pdf},
volume = {110},

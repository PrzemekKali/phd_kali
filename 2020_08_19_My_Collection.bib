doi = {10.1016/j.engstruct.2007.03.004},
file = {:C$\backslash$:/Users/PRZKALIT/Documents/Mendeley Desktop/Siringoringo, Fujino - 2008 - System identification of suspension bridge from ambient vibration response.pdf:pdf},

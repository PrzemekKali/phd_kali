title = {{Damage identification algorithm of hinged joints for simply supported slab bridges based on modified hinge plate method and artificial bee colony algorithms}},
url = {http://www.mdpi.com/1999-4893/11/12/198 http://files/485/Liu i in. - 2018 - Damage Identification Algorithm of Hinged Joints f.pdf},
volume = {11},

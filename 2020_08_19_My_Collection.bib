file = {:C$\backslash$:/Users/PRZKALIT/Documents/Mendeley Desktop/D{\"{o}}hler, Lam, Mevel - 2013 - Uncertainty quantification for modal parameters from stochastic subspace identification on multi-setup measu.pdf:pdf},
issn = {08883270},

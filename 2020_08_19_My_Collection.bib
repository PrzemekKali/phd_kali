journal = {Sensors (Switzerland)},
keywords = {Bridge,Damage detection,Energy harvesting,Frequency shifts,Scour,Singular spectrum analysis,Structural health monitoring},
language = {en},
month = {jul},
number = {11},
pages = {2572},
pmid = {31174260},
title = {{Scour damage detection and structural health monitoring of a laboratory-scaled bridge using a vibration energy harvesting device}},

title = {{A Modal Perturbation Method for Eigenvalue Problem of Non-Proportionally Damped System}},
url = {https://www.mdpi.com/2076-3417/10/1/341 http://files/1105/Pan et al. - 2020 - A Modal Perturbation Method for Eigenvalue Problem.pdf},
volume = {10},
